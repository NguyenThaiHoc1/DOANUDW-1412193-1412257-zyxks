<div class="container">
    <div class="row">
        {{#each historyAuction}}
        <div class="row" style="margin: 5px 0;">
            <div class="col-xs-12 col-sm-3 col-md-2">
                <a href="/item/{{this.proid}}">
                <img src="{{this.image1}}" class="img-responsive img-box img-thumbnail">
            </div>
            <div class="col-xs-12 col-sm-9 col-md-10">
                <div class="list-group">
                    <div class="list-group-item">
                        <div class="row-content">
                            <div class="list-group-item-heading">
                                    <h4>{{{this.proname}}}</h4>
                                </a>
                            </div>
                            <div style="color:#3681f9;font-size: 16px;">Your winning price: {{this.beatprice}} $</div>
                            <div style="font-size: 15px;"><i class="glyphicon glyphicon-time"></i> Closed at: {{this.datefinish}} </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>
