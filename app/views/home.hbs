<!-- Breadcrumb-->
<div class="row" style="margin-top: 20px;">
    <div class="col-md-4 col-ms-2" id="left-menu">
        <div class="panel panel-default" style="width: 90%;">
            <!-- Default panel contents -->
            <div class="panel-heading activeForm">
                <span class="glyphicon glyphicon-user"></span> Học Tập
            </div>
            <div class="panel-body">
                <ul class="nav" style="padding: 0 5%;" id="listAlbums">
                    <li><a href="#">Xem lịch thi</a></li>
                    <li><a href="#">Tra cứu kết quả học tập</a></li>
                </ul>
            </div>
            <div class="panel-heading activeForm">
                <span class="glyphicon glyphicon-pencil"></span> Đăng ký học phần
            </div>
            <div class="panel-body">
                <ul class="nav" style="padding: 0 5%;" id="listAlbums">
                    <li><a class="activelabel" href="#">Đăng ký học phần</a></li>
                    <li><a href="#">Kết quả đăng ký học phần</a></li>
                    <li><a href="#">Lịch sử đăng ký học phần</a></li>
                </ul>
            </div>
            <div class="panel-heading activeForm">
                <span class="glyphicon glyphicon-book"></span> Chuyên đề
            </div>
            <div class="panel-body">
                <ul class="nav" style="padding: 0 5%;" id="listAlbums">
                    <li><a href="#">Đăng ký chuyên đề</a></li>
                    <li><a href="#">Kết quả đăng ký</a></li>
                </ul>
            </div>
        </div>
    </div>
    <div class="col-md-8 col-ms-10">
        <div id="myCarousel" class="carousel slide" data-ride="carousel">
            <!-- Indicators -->
            <ol class="carousel-indicators">
                <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                <li data-target="#myCarousel" data-slide-to="1"></li>
                <li data-target="#myCarousel" data-slide-to="2"></li>
            </ol>

            <!-- Wrapper for slides -->
            <div class="carousel-inner">
                <div class="item active">
                    <img src="http://placehold.it/254x150/2ecc71/f5f5f5/&text=New+Collection" alt="Los Angeles" style="width:100%;">
                </div>

                <div class="item">
                    <img src="http://placehold.it/254x150/3498db/f5f5f5/&text=New+Collection" alt="Chicago" style="width:100%;">
                </div>

                <div class="item">
                    <img src="http://placehold.it/254x150/ff3546/f5f5f5/&text=New+Collection" alt="New york" style="width:100%;">
                </div>
            </div>

            <!-- Left and right controls -->
            <a class="left carousel-control" href="#myCarousel" data-slide="prev">
                <span class="glyphicon glyphicon-chevron-left"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="right carousel-control" href="#myCarousel" data-slide="next">
                <span class="glyphicon glyphicon-chevron-right"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </div>
</div>
